--Made By Avoidzs  On youtube https://www.youtube.com/channel/UCDnXaPXXoD715UFo15Ii5-A
local MoneyEnabled = false
local RebirthEnabled = false
local PetEnabled = false

local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3')))()

local w = library:CreateWindow("Money Clicker Simulator") -- Creates the window

local b = w:CreateFolder("AutoFarm") -- Creates the folder(U will put here your buttons,etc)

local c = w:CreateFolder("Delete UI")
c:DestroyGui()
b:Toggle("AutoMoney",function(bool)
    shared.toggle = bool
    tostring(shared.toggle)
    MoneyEnabled = bool
    end)
    
b:Toggle("AutoRebirth",function(bool)
shared.toggle = bool
RebirthEnabled = bool
end)

b:Toggle("AutoPetBuy",function(bool)
shared.toggle = bool
PetEnabled = bool
end)

while wait(0.1) do
if MoneyEnabled == true then
local args = {
    [1] = "Click"
}

game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
end
if RebirthEnabled == true then
local args = {
    [1] = "Rebirth"
}
game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
end

if PetEnabled == true then
local args = {
    [1] = "Open",
    [2] = 1,
    [3] = "Map3",
    [4] = false,
    [5] = false
}
game:GetService("ReplicatedStorage").pet:InvokeServer(unpack(args))
end
end


